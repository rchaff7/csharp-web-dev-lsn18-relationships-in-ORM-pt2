@{
}

@model List<CodingEventsDemo.Models.EventTag>

@if (Model.Count == 0)
{
    <h1>No elements with the given tag</h1>
}
else
{
    <h1>Events Tagged: @Model[0].Tag.Name</h1>

    <ul>
        @foreach (var evtTag in Model)
        {
            <li>@evtTag.Event.Name</li>
        }
    </ul>

}